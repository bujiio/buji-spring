<?xml version="1.0"?>

<project name="jsecurity-support-spring">

    <property name="root.base.dir" value="../.."/>

    <!-- Module properties -->
    <property name="submodule.name" value="spring"/>
    <property name="dist.jarName" value="jsecurity-spring.jar"/>
    <property name="dist.jar" value="${root.base.dir}/build/dist/${dist.jarName}"/>

    <import file="../../build.xml" />

    <path id="all.libs">
		<fileset dir="${lib.dir}">
			<include name="**/*.jar"/>
		</fileset>
        <fileset dir="${root.base.dir}/build/dist">
            <include name="jsecurity.jar"/>
        </fileset>
    </path>

</project>